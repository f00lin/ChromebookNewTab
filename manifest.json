{
  "name": "CB New Tab",
  "manifest_version": 2,
  "version": "2.0",
  
  "options_page": "options.html",
  
  "permissions": [
            "storage"
            ],
  
  "content_security_policy": "script-src 'self'; object-src 'self'; style-src 'self'; font-src 'self'",
    

  "chrome_url_overrides" : {
    "newtab": "info.html"
  }
}
